<% if flash[:errors] %>
  <ul>
    <% flash[:errors].each do |error_message| %>
      <li>
        <%= error_message %>
      </li>
    <% end %>
  </ul>
<% end %>


<%= form_for @song do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label "Artist" %>
    <%= f.text_field :album %>
    <%= f.label :genre %>
    <%= f.text_field :genre %>
    <%= f.submit %>
<% end %>